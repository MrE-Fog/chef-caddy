[Unit]
Description=Caddy webserver
Documentation=https://caddyserver.com/
After=network.target

[Service]
#User=some_user
#Group=some_group
WorkingDirectory=/usr/local/caddy
LimitNOFILE=4096
ExecStart=<%= @command %> <%= @options %>
Restart=on-failure
StartLimitInterval=600

[Install]
WantedBy=multi-user.target
